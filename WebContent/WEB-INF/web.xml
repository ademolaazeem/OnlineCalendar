<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <!-- start login -->
     <servlet>  
        <servlet-name>login</servlet-name>  
        <servlet-class>ie.ucd.cloudcomputing.oc.manager.LoginServlet</servlet-class>  
    </servlet>  
  
    <servlet-mapping>  
        <servlet-name>login</servlet-name>  
        <url-pattern>/loginServlet</url-pattern>  
    </servlet-mapping>  
    <!-- end login -->
    
    <!-- start search -->
    <servlet>
        <servlet-name>Search</servlet-name>
        <servlet-class>ie.ucd.cloudcomputing.oc.manager.Search</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/Search</url-pattern>
    </servlet-mapping>
   <!-- end search -->
   
   
   
   
   <servlet>
        <servlet-name>ShareCalendar</servlet-name>
        <servlet-class>ie.ucd.cloudcomputing.oc.manager.ShareCalendar</servlet-class>
    </servlet>
     <servlet-mapping>
        <servlet-name>ShareCalendar</servlet-name>
        <url-pattern>/shareNow</url-pattern>
    </servlet-mapping>
    
    
    
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>